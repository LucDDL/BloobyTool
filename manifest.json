{
  "manifest_version": 3,
  "name": "Assistente da base",
  "version": "1.0.0",
  "description": "Descrição da sua extensão",
  "icons": {
    "48": "icons/icon-48.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icons/icon-48.png"
    }
  },
  "permissions": [
    "tabs",
    "storage",
    "unlimitedStorage"
  ],
  "host_permissions": [
    "https://www.google.com/"
  ]
}

